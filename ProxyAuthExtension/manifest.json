{
  "manifest_version": 3,
  "name": "Proxy Auto Login",
  "version": "1.0",
  "description": "Automatically authenticate proxy without popup",
  "permissions": [
    "proxy",
    "webRequest",
    "webRequestAuthProvider",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}